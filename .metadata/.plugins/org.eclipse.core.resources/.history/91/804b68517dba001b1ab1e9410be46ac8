import java.util.Arrays;

public class Sortings {

	public static void main(String[] args) {
		
		int [] array= {7,2,12,8,3};
		bubbleSort(array);

	}
	
	public static void bubbleSort(int[] array)
	{
		for (int i=0;i<array.length;i++)
			
		{
			for (int j=1;j<=i;j++)
			{
				
				if (array[i]<array[j])
				{
					int temp = array[j];
					array[j]=array[i];
					array[i]=temp;
							
					
					
				}
			}
		}
		for (int i = 0; i < array.length; i++) {
			System.out.print(array[i]+" ");
			
		}
		 
	}

}
